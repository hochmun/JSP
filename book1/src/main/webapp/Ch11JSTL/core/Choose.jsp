<%@ page contentType="text/html;charset=UTF-8" pageEncoding="UTF-8"%>
<!DOCTYPE html>
<html>
	<head>
		<meta charset="UTF-8">
		<title>JSTL - choose/when/otherwise</title>
	</head>
	<body>
		<!-- 변수 선언 -->
		<c:set var="number" value="100"/>
		
		<h4>choose 태그로 홀짝 판단하기</h4>
		<c:choose>
			<c:when test="${ numb }"
	</body>
</html>